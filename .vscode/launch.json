  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/twiligo/main.go",
      "env": {
        "TWILIO_ACCOUNT_SID": "<sid>",
        "TWILIO_AUTH_TOKEN": "<token>",
        "TWILIO_PHONE_NUMBER": "<from>"
      },
      "args": ["-to", "<number>", "-msg", "<text>"]
    }
  ]
}
